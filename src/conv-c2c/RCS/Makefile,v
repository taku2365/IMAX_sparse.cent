1.9
log
@*** empty log message ***
@
text
@
## EMAX6 Compiler                      ##
##         Copyright (C) 2012 by NAIST ##
##         Primary writer: Y.Nakashima ##
##                nakashim@@is.naist.jp ##

PROGRAM	      = conv-c2c
ifeq ($(MACHTYPE),aarch64)
CFLAGS        = -O2 -fsigned-char
LDFLAGS       =
else
CFLAGS        = -O2 -m32 -fsigned-char
LDFLAGS       = -m32
endif
LIBFLAGS      =

OBJS	      = main.o \
		y.tab.o \
		emax6.o

SRCS	      = main.c \
		y.tab.c \
		emax6.c

all:	      $(PROGRAM)

$(PROGRAM):     $(OBJS)
		@@echo -n "Loading $(PROGRAM) ... "
		@@cc $(OBJS) $(LDFLAGS) -o $(PROGRAM) $(LIBFLAGS)
		@@echo "done"

.c.o:;		cc -c $(CFLAGS) $<

y.tab.c:	conv-c2c.y
		yacc conv-c2c.y

lex.yy.c:	conv-c2c.l
		lex -l conv-c2c.l

clean:;		@@rm -f $(OBJS) y.tab.c lex.yy.c core mon.out Make.log *~

###

main.o:		conv-c2c.h
y.tab.o:	conv-c2c.h emax6.h lex.yy.c
emax6.o:	conv-c2c.h emax6.h
@


